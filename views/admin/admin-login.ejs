        <h4 class="mb-2">Welcome to Materio! ðŸ‘‹</h4>
        <p class="mb-4">Please sign-in to your account and start the adventure</p>

        <div class="login-container">
          <div class="login-card">
           
            
            <% if ( message) { %>
              <div class="alert alert-danger text-center">
                  <%= message %>
              </div>
          <% } %>
            
            <form id="adminLoginForm" method="post" action="/admin/login">
              <div class="form-group mb-4">
                <label for="adminEmail" class="form-label">Email address</label>
                <div class="input-group">
                 
                  <input
                    type="email"
                    class="form-control"
                    id="adminEmail"
                    name="email"
                    placeholder="Enter your email"
                    required
                  />
                </div>
                <div id="emailError" class="error-message mt-1"></div>
              </div>
              
              <div class="form-group mb-4">
                <label for="adminPassword" class="form-label">Password</label>
                <div class="input-group">
                  
                  <input
                    type="password"
                    class="form-control"
                    id="adminPassword"
                    name="password"
                    placeholder="Enter your password"
                    required
                  />
                  <span class="input-group-text cursor-pointer toggle-password">
                    <i class="ri-eye-off-line"></i>
                  </span>
                </div>
                <div id="passwordError" class="error-message mt-1"></div>
              </div>
              
              <div class="form-check mb-4">
                <input class="form-check-input" type="checkbox" id="rememberMe">
                <label class="form-check-label" for="rememberMe">
                  Remember Me
                </label>
              </div>
              
              <button type="submit" class="btn btn-primary btn-block w-100 mb-3">Log In</button>
              
              <div class="text-center">
                <a href="/forgot-password" class="text-muted">Forgot Password?</a>
              </div>
            </form>
          </div>
        </div>


        

        <p class="text-center">
          <span>New on our platform?</span>
          <a href="auth-register-cover.html">
            <span>Create an account</span>
          </a>
        </p>

        <div class="divider my-4">
          <div class="divider-text">or</div>
        </div>

